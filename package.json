{
  "name": "ngis",
  "version": "0.0.1",
  "description": "Minimalistic browser GIS",
  "main": "index.js",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (http://github.com/stepankuzmin)",
  "license": "MIT",
  "engines": {
    "node": ">=0.11"
  },
  "scripts": {
    "start": "NODE_ENV=development node index.js",
    "lint": "eslint src test",
    "test": "babel-tape-runner test/test.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "syntax-async-functions",
            "transform-runtime"
          ],
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-hmr",
                  "imports": [
                    "react"
                  ],
                  "locals": [
                    "module"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "babel-plugin-syntax-async-functions": "6.3.13",
    "babel-polyfill": "6.3.14",
    "babel-register": "6.3.13",
    "immutable": "3.7.6",
    "jade": "1.11.0",
    "koa": "1.1.2",
    "koa-bodyparser": "2.0.1",
    "koa-compose": "2.3.0",
    "koa-cors": "0.0.16",
    "koa-favicon": "1.2.0",
    "koa-jwt": "1.1.1",
    "koa-morgan": "0.4.0",
    "koa-mount": "1.3.0",
    "koa-router": "5.3.0",
    "koa-validate": "0.2.11",
    "minimist": "1.2.0",
    "r-dom": "2.1.0",
    "ramda": "0.19.1",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-immutable-proptypes": "1.5.1"
  },
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-eslint": "5.0.0-beta6",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-runtime": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-register": "6.3.13",
    "babel-tape-runner": "2.0.0",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "3.1.0",
    "eslint-loader": "1.2.0",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-import": "0.12.1",
    "eslint-plugin-react": "3.14.0",
    "json-loader": "0.5.4",
    "koa-webpack-dev-middleware": "1.1.0",
    "koa-webpack-hot-middleware": "1.0.3",
    "pre-commit": "1.1.2",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "supertest-koa-agent": "0.2.1",
    "tape": "4.4.0",
    "webpack": "1.12.10"
  }
}
